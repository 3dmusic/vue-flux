{
  "name": "vue-flux",
  "version": "6.3.0",
  "description": "Vue image slider",
  "author": "ragnar lotus",
  "repository": {
    "type": "git",
    "url": "https://github.com/ragnarlotus/vue-flux"
  },
  "keywords": [
    "vue",
    "image",
    "slider",
    "carousel",
    "parallax"
  ],
  "license": "MIT",
  "bugs": "https://github.com/ragnarlotus/vue-flux/issues",
  "homepage": "https://ragnarlotus.github.com/vue-flux-docs",
  "main": "dist/vue-flux.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run bundle:nossr && npm run bundle:ssr",
    "bundle": "vue-cli-service build --target lib --name vue-flux \"src/components/index.js\"",
    "bundle:nossr": "set EXTRACT_CSS=false&& npm run bundle -- --dest dist",
    "bundle:ssr": "set EXTRACT_CSS=true&& npm run bundle -- --dest dist-ssr",
    "lint": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit --watchAll --coverage"
  },
  "files": [
    "dist",
    "dist-ssr"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.8",
    "@vue/cli-plugin-unit-jest": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "@vue/test-utils": "~1.1.1",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.5.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-vue": "^7.1.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.0.5",
    "tailwindcss": "^1.5.2",
    "vue": "^2.6.11",
    "vue-cosk": "^0.2.5",
    "vue-template-compiler": "^2.6.11"
  }
}
