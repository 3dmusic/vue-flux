{
  "name": "vue-flux",
  "version": "6.1.0",
  "description": "Vue image slider",
  "author": "ragnar lotus",
  "repository": {
    "type": "git",
    "url": "https://github.com/deulos/vue-flux"
  },
  "keywords": [
    "vue",
    "image",
    "slider",
    "carousel",
    "parallax"
  ],
  "license": "MIT",
  "bugs": "https://github.com/deulos/vue-flux/issues",
  "homepage": "https://deulos.github.com/vue-flux-docs",
  "main": "dist/vue-flux.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run bundle:nossr && npm run bundle:ssr",
    "bundle": "vue-cli-service build --target lib --name vue-flux \"src/components/index.js\"",
    "bundle:nossr": "set EXTRACT_CSS=false&& npm run bundle -- --dest dist",
    "bundle:ssr": "set EXTRACT_CSS=true&& npm run bundle -- --dest dist-ssr",
    "lint": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit"
  },
  "files": [
    "dist",
    "dist-ssr"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^6.3.0",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^1.1.2",
    "vue": "^2.6.10",
    "vue-cosk": "0.2.1",
    "vue-template-compiler": "^2.6.10"
  }
}
